n        id: Date.now(),\n        title,\n        done: false\n      };\n      if (this.title !== \"\") {\n        this.$emit(\"newItem\", newTodo);\n        this.title = \"\";\n      }\n    }\n  },\n\n  computed: {\n    remain() {\n      var i = this.todos.filter(todo => !todo.done).length;\n      return i;\n    },\n    word() {\n      var i = this.todos.filter(todo => !todo.done).length;\n      if (i <= 1) {\n        return \"todo\";\n      } else {\n        return \"todos\";\n      }\n    }\n  }\n};\n</script>\n\n<style>\n</style>"]}]}